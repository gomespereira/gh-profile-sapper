{
  "name": "gh-profile-sapper",
  "version": "1.0.0",
  "scripts": {
    "watch:tailwind": "postcss -w src/tailwind.css -o static/global.css",
    "build:tailwind": "postcss src/tailwind.css -o static/global.css",
    "dev:sapper": "sapper dev",
    "dev": "run-p watch:tailwind dev:sapper",
    "prebuild": "npm run build:tailwind",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "polka": "^0.5.2",
    "sirv": "^1.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/runtime": "^7.11.2",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^5.0.2",
    "@tsconfig/svelte": "^1.0.10",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postcss-preset-env": "^6.7.0",
    "rollup": "^2.26.6",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.2",
    "svelte": "^3.24.1",
    "svelte-check": "^1.0.27",
    "svelte-preprocess": "^4.1.1",
    "tailwindcss": "^1.7.5",
    "typescript": "^3.9.7"
  }
}
